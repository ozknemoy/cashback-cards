    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/node": "^8.0.30",
    "autoprefixer": "^8.1.0",
    "browser-sync": "^2.23.6",
    "codelyzer": "^4.0.2",
    "connect-history-api-fallback": "^1.5.0",
    "cpy-cli": "^1.0.1",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.2",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "http-server": "^0.10.0",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.10",
    "run-sequence": "^2.2.1",
